          "import_as": "validator"
        }
      ],
      "mod_dependencies": [
        "echo-logger-wrapper",
        "validator"
      ],
      "config": [
        {
          "key": "EchoServerPort",
          "type": "int",
          "value": "9000",
          "struct_tag": "envconfig:\"ECHO_SERVER_PORT\" default:\"9000\"",
          "env_key": "ECHO_SERVER_PORT"
        }
      ]
    }
